IF Y10Out_waitUntilNextStep.Q = TRUE THEN
	Y10Out_waitUntilNextStep.IN := FALSE;
	Y10Out_waitUntilNextStep();
	
	THIS^.stateMachineState := mainStateMachineStatus_t.CONVAYORBELT_START; // move on to next step
END_IF
]]></ST>
      </Implementation>
    </Method>
