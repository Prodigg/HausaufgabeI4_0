    <Method Name="GripperUpLeft" Id="{764b5797-fcfa-46a1-9cde-c148855433ab}" FolderPath="states\">
      <Declaration><![CDATA[METHOD PRIVATE GripperUpLeft : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
